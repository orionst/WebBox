<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="center" prefHeight="400.0" prefWidth="400.0" stylesheets="@/fxml/boxstyle.css" xmlns="http://javafx.com/javafx/8.0.92" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainController">
         <children>
            <HBox prefHeight="423.0" prefWidth="400.0">
               <children>
            <TreeView prefHeight="312.0" prefWidth="429.0" />
                  <VBox prefHeight="312.0" prefWidth="230.0">
                     <children>
                        <Button fx:id="sendButton" mnemonicParsing="false" onAction="#sendFileButtonAction" text="Загрузить файл" />
                        <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#deleteFileButtonAction" prefHeight="25.0" prefWidth="111.0" text="Удалить файл" />
                        <Button fx:id="settingsButton" mnemonicParsing="false" onAction="#openSettingsWindowButtonAction" prefHeight="25.0" prefWidth="127.0" text="Настройки" />
                     </children>
                  </VBox>
               </children>
            </HBox>
      <Pane prefHeight="200.0" prefWidth="200.0">
         <children>
            <Label layoutX="4.0" text="Server:" />
            <Label layoutX="4.0" layoutY="17.0" text="Label" />
         </children>
      </Pane>
         </children>
      </VBox>
