<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="178.0" prefWidth="341.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="SettingsWindowController">
   <children>
      <VBox prefHeight="172.0" prefWidth="335.0">
         <children>
            <Group VBox.vgrow="ALWAYS">
                <children>
                   <Label layoutX="9.0" layoutY="11.0" text="Server:" />
                    <TextField id="serverName" fx:id="serverName" layoutX="54.0" layoutY="7.0" />
                    <Label layoutX="212.0" layoutY="11.0" text="Port" />
                    <TextField id="serverPort" fx:id="serverPort" layoutX="242.0" layoutY="7.0" prefHeight="25.0" prefWidth="68.0" />
                </children>
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </Group>
            <Group VBox.vgrow="ALWAYS">
                <children>
                    <Label layoutX="-1.0" layoutY="-2.0" text="User Account:">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font></Label>
                    <Label layoutY="24.0" text="User:" />
                    <TextField fx:id="userName" layoutX="34.0" layoutY="20.0" prefHeight="25.0" prefWidth="122.0" />
                    <Label layoutX="162.0" layoutY="23.0" text="Login:" />
                    <PasswordField fx:id="userPass" layoutX="202.0" layoutY="20.0" prefHeight="25.0" prefWidth="122.0" />
                    <Button layoutX="248.0" layoutY="53.0" mnemonicParsing="false" onAction="#signUpAction" prefHeight="25.0" prefWidth="74.0" text="Sign Up" />
                    <Button layoutX="171.0" layoutY="53.0" mnemonicParsing="false" onAction="#signInAction" prefHeight="25.0" prefWidth="68.0" text="Sign In" />
                    <Label layoutY="82.0" prefHeight="17.0" prefWidth="317.0" text="Label" textFill="RED" />
                    <CheckBox fx:id="autoLogon" layoutX="34.0" layoutY="57.0" mnemonicParsing="false" text="Auto logon" />
                </children>
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>

            </Group>
            <Pane prefHeight="29.0" prefWidth="335.0">
               <children>
                    <Button fx:id="okButton" layoutX="224.0" layoutY="2.0" mnemonicParsing="false" onAction="#okAction" prefHeight="25.0" prefWidth="52.0" text="OK" />
                    <Button fx:id="cancelButton" layoutX="283.0" layoutY="2.0" mnemonicParsing="false" onAction="#cancelAction" text="Cancel" />
               </children>
               <opaqueInsets>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </opaqueInsets>
            </Pane>
         </children>
      </VBox>
   </children>
</AnchorPane>
